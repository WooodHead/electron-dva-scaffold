{
  "name": "electron-tools",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config ./scripts/build/out/webpack.config.renderer.dev.dll.js --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config ./scripts/build/out/webpack.config.main.prod --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config ./scripts/build/out/webpack.config.renderer.prod --colors",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./scripts/build/out/webpack.config.renderer.dev.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./src/main",
    "start": "cross-env NODE_ENV=production electron .",
    "dev": "cross-env START_HOT=1 node -r babel-register ./scripts/build/out/utils/checkPortInUse.js && cross-env START_HOT=1 npm run start-renderer-dev",
    "package": "node ./scripts/package"
  },
  "author": "xzper",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.4.1",
    "classnames": "^2.2.5",
    "dva": "^2.2.2",
    "dva-loading": "^2.0.0",
    "exceljs": "^1.1.3",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.22.1",
    "nprogress": "^0.2.0",
    "number2english": "0.0.1",
    "query-string": "^6.0.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/detect-port": "^1.1.0",
    "@types/exceljs": "^0.5.2",
    "@types/extract-text-webpack-plugin": "^3.0.2",
    "@types/fs-extra": "^5.0.2",
    "@types/lodash": "^4.14.53",
    "@types/node": "^7.0.5",
    "@types/nprogress": "0.0.29",
    "@types/query-string": "^5.1.0",
    "@types/react": "^15.0.12",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^5.0.16",
    "@types/react-router-dom": "^4.2.6",
    "@types/systemjs": "^0.20.6",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.1.3",
    "@types/webpack-bundle-analyzer": "^2.9.2",
    "@types/webpack-merge": "^4.1.3",
    "awesome-typescript-loader": "^5.0.0-1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.0",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "detect-port": "^1.2.2",
    "electron": "1.8.4",
    "electron-packager": "^12.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^0.9.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "postcss-loader": "^2.1.4",
    "style-loader": "^0.21.0",
    "svg-sprite-loader": "0.0.26",
    "ts-node": "^4.1.0",
    "tslint": "^3.14.0",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.5.7",
    "webpack": "^4.5.0",
